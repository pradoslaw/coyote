{
    "name": "adam-boduch/coyote",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/adam-boduch/geshi"
        },
        {
            "type": "vcs",
            "url": "https://github.com/chrisboulton/php-diff"
        }
    ],
    "description": "4programmers.net",
    "version": "2.3",
    "homepage": "http://4programmers.net",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Adam Boduch",
            "email": "adam@boduch.net"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-gd": "*",
        "ext-json": "*",
        "adam-boduch/geshi": "dev-master",
        "adam-boduch/laravel-grid": "dev-master",
        "barryvdh/laravel-debugbar": "^3.2",
        "chrisboulton/php-diff": "dev-master",
        "doctrine/dbal": "^2.5",
        "elasticsearch/elasticsearch": "~5.0",
        "erusev/parsedown": "^1.7",
        "ezyang/htmlpurifier": "^4.7",
        "fideloper/proxy": "^4.0",
        "filp/whoops": "^2.3",
        "florianv/laravel-swap": "^1.1",
        "guzzlehttp/guzzle": "^6.2",
        "guzzlehttp/oauth-subscriber": "^0.3.0",
        "inacho/php-credit-card-validator": "^1.0",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "jenssegers/agent": "^2.3",
        "laravel-notification-channels/twilio": "^2.0",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.2",
        "laravel/socialite": "^3.0",
        "laravelcollective/html": "5.7.*",
        "lavary/laravel-menu": "1.7.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "mpdf/mpdf": "dev-development",
        "paylane/client": "^1.1",
        "pda/pheanstalk": "^3.1",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message": "^1.5",
        "predis/predis": "^1.0",
        "rcrowe/twigbridge": "0.9.*",
        "sentry/sentry-laravel": "^1.1",
        "twig/extensions": "^1.3"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.5",
        "codeception/codeception": "^3.0",
        "symfony/css-selector": "4.2.*",
        "symfony/dom-crawler": "4.2.*",
        "mockery/mockery": "^1.0",
        "codeception/mockery-module": "^0.3",
        "laravel/dusk": "^5.0",
        "itsgoingd/clockwork": "^4.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database",
            "app/Models",
            "app/Plugins"
        ],
        "psr-4": {
            "Coyote\\": "app/"
        },
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
